      } else if (a % 3 == 0) {\n            a /= 3;\n        } else if (a % 4 == 0) {\n            a *= 2;\n        }\n    }\n\n    function ifWithElseIfElse() public {\n        if (a >= 100) {\n            a = 100;\n        } else if (a < 50) {\n            a = 80;\n        } else {\n            a = 75;\n        }\n    }\n}"}}