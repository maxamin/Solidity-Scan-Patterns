      bytes32 s;\n        ecrecover(hash,v,r,s);\n    }\n\n    function address_related() public{\n        address a;\n        a.balance;\n        a.send(0);\n    }\n\n    function contract_related() public{\n        this;\n        address a;\n        selfdestruct(a);\n    }\n}\n"}}